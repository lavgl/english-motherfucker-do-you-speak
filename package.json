{
  "name": "english-motherfucker-do-you-speak",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://lavgl.github.io/english-motherfucker-do-you-speak",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-jest": "^23.0.0-alpha.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^23.0.0-alpha.0",
    "jest-environment-enzyme": "^6.0.0",
    "jest-enzyme": "^6.0.0",
    "jest-puppeteer-preset": "^2.0.1",
    "prettier": "1.11.1",
    "puppeteer": "^1.2.0"
  },
  "jest": {
    "projects": [
      {
        "displayName": "jsdom",
        "testMatch": ["<rootDir>/**/*.spec.js"],
        "moduleNameMapper": {
          ".+\\.(svg|css)": "identity-obj-proxy"
        },
        "setupTestFrameworkScriptFile": "<rootDir>/test/helpers/setupEnzyme.js"
      },
      {
        "displayName": "e2e",
        "testMatch": ["<rootDir>/test/e2e/**/*.e2e.js"],
        "globalSetup": "jest-environment-puppeteer/setup",
        "globalTeardown": "jest-environment-puppeteer/teardown",
        "testEnvironment": "jest-environment-puppeteer",
        "setupTestFrameworkScriptFile": "expect-puppeteer"
      }
    ],
    "preset": "jest-puppeteer-preset"
  }
}
